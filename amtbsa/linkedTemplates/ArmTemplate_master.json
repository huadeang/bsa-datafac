{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "amtbsa"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureSqlBillingDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlBillingDatabase'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"LocalFileServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LocalFileServer'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://aksvault01.vault.azure.net/"
		},
		"DemoBillingFileServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\credit-control\\review"
		},
		"DemoBillingFileServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "sangad"
		},
		"LocalFileServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\credit-control"
		},
		"LocalFileServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "sangad"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "amtbsa_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlBillingDatabase_connectionString": {
						"value": "[parameters('AzureSqlBillingDatabase_connectionString')]"
					},
					"LocalFileServer_password": {
						"value": "[parameters('LocalFileServer_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"DemoBillingFileServer_properties_typeProperties_host": {
						"value": "[parameters('DemoBillingFileServer_properties_typeProperties_host')]"
					},
					"DemoBillingFileServer_properties_typeProperties_userId": {
						"value": "[parameters('DemoBillingFileServer_properties_typeProperties_userId')]"
					},
					"LocalFileServer_properties_typeProperties_host": {
						"value": "[parameters('LocalFileServer_properties_typeProperties_host')]"
					},
					"LocalFileServer_properties_typeProperties_userId": {
						"value": "[parameters('LocalFileServer_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}